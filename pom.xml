    <groupId>com.zj.example.keycloak</groupId>
    <artifactId>keycloak-admin-client-test</artifactId>
    <version>1.0-SNAPSHOT</version>

    <properties>
        <keycloak.version>1.7.0.Final</keycloak.version>
